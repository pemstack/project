{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "src",
    "declaration": false,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "noEmit": true,
    "target": "es5",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "./src"
  ]
}
