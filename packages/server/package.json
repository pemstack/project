    "@nestjs/platform-express": "^6.8.2",
    "@nestjs/serve-static": "^1.0.1",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
